//Push
class Node{
    constructor(val){
        this.val=val;
        this.next=null
        
    }
}

// var first=new Node("Hi");
// first.next=new Node("Hello");
// first.next.next=new Node("How");
// first.next.next.next=new Node("are");
// first.next.next.next.next=new Node("you");

class singlyLinkedList{
    constructor(){
        this.head=null
        this.tail=null
        this.length=0
        
    }

    push(val){
        var newNode= new Node(val)
        if(!this.head){
            this.head=newNode;
            this.tail=this.head
        }else{
            this.tail.next=newNode
            this.tail=newNode
        }
        this.length++;
        return this
    }
}

var list = new singlyLinkedList()
list.push("Hello");
list.push("How");
list.push("are")
